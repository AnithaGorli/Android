<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.animations"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="21" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity android:name="com.pcs.animations.MainActivity">
            <intent-filter >
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="com.pcs.animations.SlidingPageActivity"></activity>
        <activity android:name="com.pcs.animations.ZoomActivity"></activity>
        <activity android:name="com.pcs.animations.BlinkActivity"></activity>
        <activity android:name="com.pcs.animations.FadeActivity"></activity>
          <activity android:name="com.pcs.animations.RotateActivity"></activity>
            <activity android:name="com.pcs.animations.Bounce"></activity>
    </application>

</manifest>
